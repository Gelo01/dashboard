<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>
  <body>
    <%- include('../partials/header'); %>

    <main>
      <section>
        <div class="container">
          <div class="row">
            <div class="col-12">
              <h1>Login Page</h1>
              <p><%= msg %></p>

              <ul>
                <% accounts.forEach(function(d) { %>
                <li>
                  <strong>Name: <%= d.username %> / <%= d.email %></strong>
                </li>
                <% }); %>
              </ul>
            </div>
          </div>
        </div>
      </section>
    </main>

    <%- include('../partials/footer'); %>
  </body>
</html>
